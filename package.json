    "dev": "webpack-dev-server --watch",
    "build": "webpack --mode production",
    "deploy": "node utils/scripts/deploy.js",
    "lint": "eslint --ext .js src",
    "lint:fix": "npm run lint -- --fix",
    "release": "standard-version",
    "template": "node utils/scripts/makeTemplate.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GPortfolio/GPortfolio.git"
  },
  "keywords": [
    "github-portfolio",
    "gitportfolio",
    "gportfolio",
    "portfolio",
    "github",
    "git",
    "projects",
    "cv",
    "resume",
    "open-source"
  ],
  "author": "Alexey Khrushch, alexeykhr@outlook.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GPortfolio/GPortfolio/issues"
  },
  "homepage": "https://github.com/GPortfolio/GPortfolio#readme",
  "dependencies": {
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.0.0",
    "ejs-loader": "^0.3.3",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.2",
    "file-loader": "^4.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "shelljs": "^0.8.3",
    "svg-inline-loader": "^0.8.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.2",
    "webpack-pwa-manifest": "^4.0.0",
    "workbox-webpack-plugin": "^4.3.1"
  }
